<?xml version='1.0'?>
<config type='HEX4' FlightController='pixhawk'>
    <entry index="0" name="Cloud">
        <description>Cloud param.</description>
        <param index="1" value="1"    unit='bool'>Whether is cloud module? 0:close 1:open</param>
        <param index="2" value="queue.airforceuav.com" unit="Domain/IP">MQTT IP Adress</param>
        <param index="3" value="1883" unit="No.">MATT Port Number </param>      
    </entry>
    <entry index="1" name="AIL">
        <description>Aileron PWM.</description>
        <param index="1" value="1"    unit="No.">Aileron Channel Number</param>
        <param index="2" value="723"  unit="PWM">LOW AIL PWM </param>
        <param index="3" value="956"  unit="PWM">MID AIL PWM </param>
        <param index="4" value="1183" unit="PWM">HIG AIL PWM </param>
        <param index="5" value="1"    unit='bool'>Positive(1) or Reverse(-1).</param>
        <param index="6" value="10"   unit="%">Variation Rate.</param>
    </entry>
    <entry index="2" name="ELE">
        <description>Elevator PWM.</description>
        <param index="1" value="2"    unit="No.">Elevator Channel Number</param>
        <param index="2" value="723"  unit="PWM">LOW ELE PWM </param>
        <param index="3" value="956"  unit="PWM">MID ELE PWM </param>
        <param index="4" value="1183" unit="PWM">HIG ELE PWM </param>
        <param index="5" value="-1"   unit='bool'>Positive(1) or Reverse(-1).</param>
        <param index="6" value="10"   unit="%">Variation Rate.</param>
    </entry>
    <entry index="3" name="THR">
        <description>Throttle PWM.</description>
        <param index="1" value="3"    unit="No.">Throttle Channel Number</param>
        <param index="2" value="723"  unit="PWM">LOW THR PWM </param>
        <param index="3" value="956"  unit="PWM">MID THR PWM </param>
        <param index="4" value="1183" unit="PWM">HIG THR PWM </param>
        <param index="5" value="1"    unit='bool'>Positive(1) or Reverse(-1).</param>
        <param index="6" value="10"   unit="%">Variation Rate.</param>
    </entry>
    <entry index="4" name="RUD">
        <description>Rudder PWM.</description>
        <param index="1" value="4"    unit="No.">Rudder Channel Number</param>
        <param index="2" value="723"  unit="PWM">LOW RUD PWM </param>
        <param index="3" value="956"  unit="PWM">MID RUD PWM </param>
        <param index="4" value="1183" unit="PWM">HIG RUD PWM </param>
        <param index="5" value="-1"   unit='bool'>Positive(1) or Reverse(-1).</param>
        <param index="6" value="10"   unit="%">Variation Rate.</param>
    </entry>
    <entry index="5" name="PIT">
        <description>Pitch PWM.</description>
        <param index="1" value="8" unit="No.">Pitch Channel Number</param>
        <param index="2" value=""  unit="PWM">LOW PIT PWM. </param>
        <param index="3" value=""  unit="PWM">MID PIT PWM.</param>
        <param index="4" value=""  unit="PWM">HIG PIT PWM.</param>
        <param index="5" value=""  unit='bool'>Positive(1) or Reverse(-1).</param>
        <param index="6" value=""  unit="%">Variation Rate.</param>
    </entry>
    <entry index="6" name="Mode">
        <description>Flight Mode.</description>
        <param index="1" value="5"   unit="No.">Flight Mode Channel Number.</param>
        <param index="2" value="954" unit="PWM">Loiter mode PWM.</param>
    </entry>
    <entry index="7" name="MCU">
        <description>MCU module.</description>
        <param index="1" value="0"     unit='bool'>Whether is MCU_module open? 0:close 1:open  </param>
        <param index="2" value="/dev/ttyUSB0" unit="Device">Serial port name</param>
        <param index="3" value="57600" unit='Baudrate'>Baudrate. </param>
        <param index="4" value="10"    unit="times">ARQ times. </param>
    </entry>
    <entry index="8" name="GPS">
        <description>GPS_module.</description>
        <param index="1" value="0"    unit='bool'>Whether is GPS_module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0" unit="Device">Serial port name</param>
        <param index="3" value="9600" unit='Baudrate'>Baudrate. </param>
        <param index="4" value="10"   unit="times">ARQ times. </param>
    </entry>
    <entry index="9" name="compass">
        <description>Compass_module.</description>
        <param index="1" value="0"    unit='bool'>Whether is Compass_module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0" unit="Device">Serial port name</param>
        <param index="3" value="9600" unit='Baudrate'>Baudrate. </param>
        <param index="4" value="10"   unit="times">ARQ times. </param>
    </entry>
    <entry index="10" name="Barometer">
        <description>Barometer module.</description>
        <param index="1" value="0"    unit='bool'>Whether is Barometer_module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0" unit="Device">Serial port name</param>
        <param index="3" value="9600" unit='Baudrate'>Baudrate. </param>
        <param index="4" value="10"   unit="times">ARQ times. </param>
    </entry>
    <entry index="11" name="Lidar">
        <description>Lidar module.</description>
        <param index="1" value="0">Whether is Lidar module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0" unit="Device">Serial port name</param>
        <param index="3" value="1500" unit="mm">Safety    Distance. </param>
        <param index="4" value="3000" unit="mm">Detection Distance. </param>
    </entry>
    <entry index="12" name="Gears">
        <description>Vehicle Gears.</description>
        <param index="1" value="1"  unit="Gear"> 1:'L', 2:'M', 3:'H'</param>
        <param index="2" value="10" unit="%">Gear 'L' rate</param>
        <param index="3" value="15" unit="%">Gear 'M' rate. </param>
        <param index="4" value="30" unit="%">Gear 'H' rate. </param>       
    </entry>
    <entry index="13" name="Movement Duration">
        <description>Movement Duration.</description>
        <param index="1" value="1" unit="seconds">Gear 'L' Duration.</param>
        <param index="2" value="1" unit="seconds">Gear 'M' Duration.</param>
        <param index="3" value="1" unit="seconds">Gear 'H' Duration.</param>
    </entry>
    <entry index="14" name="Brake Duration">
        <description>Brake Duration.</description>
        <param index="1" value="1" unit="seconds">Gear 'L' Duration</param>
        <param index="2" value="1" unit="seconds">Gear 'M' Duration.</param>
        <param index="3" value="1" unit="seconds">Gear 'H' Duration.</param>
    </entry>
    <entry index="15" name="Decision Duration">
        <description>Decision Duration.</description>
        <param index="1" value="1" unit="seconds">Gear 'L' Duration</param>
        <param index="2" value="1" unit="seconds">Gear 'M' Duration.</param>
        <param index="3" value="1" unit="seconds">Gear 'H' Duration.</param>
    </entry>
</config>
