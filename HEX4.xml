<?xml version='1.0'?>
<config type='HEX4' FlightController='Pixhawk'>
    <entry index="0" name="Cloud">
        <description>Cloud param.</description>
        <param index="1" value="1">Whether is cloud module? 0:close 1:open</param>
        <param index="2" value="123.57.44.174">MQTT IP Adress</param>
        <param index="3" value="1883">MATT Port Number </param>      
    </entry>
    <entry index="1" name="AIL">
        <description>Aileron PWM.</description>
        <param index="1" value="1">Aileron Channel Number</param>
        <param index="2" value="723">LOW AIL PWM </param>
        <param index="3" value="954">MID AIL PWM </param>
        <param index="4" value="1186">HIG AIL PWM </param>
        <param index="5" value="45">Variation </param>
    </entry>
    <entry index="2" name="ELE">
        <description>Elevator PWM.</description>
        <param index="1" value="2">Elevator Channel Number</param>
        <param index="2" value="723">LOW ELE PWM </param>
        <param index="3" value="954">MID ELE PWM </param>
        <param index="4" value="1186">HIG ELE PWM </param>
        <param index="5" value="45">Variation </param>
    </entry>
    <entry index="3" name="THR">
        <description>Throttle PWM.</description>
        <param index="1" value="3">Throttle Channel Number</param>
        <param index="2" value="723">LOW THR PWM </param>
        <param index="3" value="954">MID THR PWM </param>
        <param index="4" value="1186">HIG THR PWM </param>
        <param index="5" value="45">Variation </param>
    </entry>
    <entry index="4" name="RUD">
        <description>Rudder PWM.</description>
        <param index="1" value="4">Rudder Channel Number</param>
        <param index="2" value="723">LOW RUD PWM </param>
        <param index="3" value="954">MID RUD PWM </param>
        <param index="4" value="1186">HIG RUD PWM </param>
        <param index="5" value="45">Variation </param>
    </entry>
    <entry index="5" name="PIT">
        <description>Pitch PWM.</description>
        <param index="1" value="8">Pitch Channel Number</param>
        <param index="2" value="0">LOW PIT PWM.</param>
        <param index="3" value="0">MID PIT PWM.</param>
        <param index="4" value="0">HIG PIT PWM.</param>
        <param index="5" value="20">Variation </param>
    </entry>
    <entry index="6" name="Mode">
        <description>Flight Mode.</description>
        <param index="1" value="5">Flight Mode Channel Number.</param>
        <param index="2" value="954">PosHold mode PWM.</param>
    </entry>
    <entry index="7" name="MCU">
        <description>MCU module.</description>
        <param index="1" value="1">Whether is MCU_module open? 0:close 1:open  </param>
        <param index="2" value="/dev/ttyUSB0">Serial port</param>
        <param index="3" value="57600">Baudrate. </param>
    </entry>
    <entry index="8" name="GPS">
        <description>GPS_module.</description>
        <param index="1" value="0">Whether is GPS_module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0">Serial port</param>
        <param index="3" value="9600">Baudrate. </param>
    </entry>
    <entry index="9" name="compass">
        <description>Compass_module.</description>
        <param index="1" value="0">Whether is Compass_module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0">Serial port</param>
        <param index="3" value="9600">Baudrate. </param>
    </entry>
    <entry index="10" name="Barometer">
        <description>Barometer module.</description>
        <param index="1" value="0">Whether is Barometer_module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0">Serial port</param>
        <param index="3" value="9600">Baudrate. </param>
    </entry>
    <entry index="11" name="Lidar">
        <description>Lidar module.</description>
        <param index="1" value="0">Whether is Lidar module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0">Serial port</param>
        <param index="3" value="1500">Safety Distance. </param>
        <param index="4" value="3000">Detection Distance. </param>
    </entry>
</config>
